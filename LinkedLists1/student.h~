#ifndef STUDENT_H
#define STUDENT_H
#include <iostream>

using namespace std;

class Student{
 public:
  Student();
  Student(int inid, float ingpa, char* inname);
  void setID();
  void getID();
  void setGPA();
  void getGPA();
  void setName();
  void getName();
  void stuPrint();
 private:
  int id;
  float gpa;
  char* name;
};
#endif
